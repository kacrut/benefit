@model IEnumerable<benefit.Models.vwRekapBuListInfoBenefit>


<div class="row span4">
    <h4 class="text-center">Benefit Production VS Upload</h4><br />
    <table class="table table-striped table-bordered">
        <thead>
            <tr>
                <th>
                    NMKC
                </th>
                <th>
                    TotalProduction
                </th>
                <th>
                    TotalUpload
                </th>
            </tr>
        </thead>
    <tbody>
    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NMKC)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TotalProduction)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TotalUpload)
            </td>
        </tr>
    }
    </tbody>

    </table>
</div>
